<h1>ユーザー一覧</h1>
<table class="table">
 <thead>
  <tr>
   <th>image</th>
   <th>name</th>
  </tr>
</thead>
<tbody>
 <% @users.each do |user| %>
  <tr>
   <td><%= image_tag user.get_profile_image %></td>
   <td><%= user.name %></td>
   <td><%= link_to "Show", user_path(user.id) %></td>
  </tr>
  <% end %>
</tbody>
</table>